    "openApiV2BaseUrl": "https://openapi.tidal.com/v2",
    "authApiBaseUrl": "https://auth.tidal.com/v1",
    "privateApiV1BaseUrl": "https://tidal.com/v1",
    "privateApiV2BaseUrl": "https://tidal.com/v2",
    "resourcesBaseUrl": "https://resources.tidal.com"
}
