{
    "_version": 4,
    "defaultTypeOptions": {
        "directory": "./Downloads/{{albumArtist.name}}/{{album.title}} ({{releaseYear}})",
        "filename": "{{artist.name}} - {{album.title}} - {{title}} - {{trackNumberPadded}}",
        "coverFilename": "cover"
    },
    "typeOptions": {
        "video": {
            "directory": "./Downloads/{{artist.name}}/{{title}} ({{releaseYear}})",
            "filename": "{{artist.name}} - {{title}}"
        },
        "playlist": {
            "directory": "./Downloads/Playlists/{{playlist.title}}",
            "filename": "{{playlistItemNum}} - {{artist.name}} - {{title}}",
            "coverFilename": null
        }
    },
    "trackQuality": "MAX",
    "videoQuality": "MAX",
    "playlistCoverFilename": "cover",
    "createPlaylistFile": true,
    "useDolbyAtmos": false,
    "getLyrics": true,
    "syncedLyricsOnly": false,
    "plainLyricsOnly": false,
    "externalLyrics": false,
    "embedMetadata": true,
    "artistTagSeparator": "; ",
    "roleTagSeparator": ";",
    "useArtistsTag": false,
    "allowUserUploads": false,
    "partialDataFallback": true,
    "getCover": true,
    "trackCoverSize": "1280",
    "videoCoverSize": "1280x720",
    "playlistCoverSize": "ORIGINAL",
    "customMetadata": [
        ["comment", "Downloaded from TIDAL: {{url}}"],
        ["source", "TIDAL"],
        ["tidal_albumid", "{{album.id}}"],
        ["tidal_albumartistid", "{{albumArtist.id}}"],
        ["tidal_trackid", "{{track.id}}"],
        ["tidal_videoid", "{{video.id}}"],
        ["tidal_artistid", "{{artist.id}}"]
    ],
    "metadataEmbedder": "ffmpeg",
    "downloadLogPadding": 70,
    "overwriteExisting": false,
    "segmentWaitMin": 50,
    "segmentWaitMax": 50,

    "ffmpegPath": "ffmpeg",
    "kid3CliPath": "kid3-cli",
    "secretsPath": "./secrets.json",

    "debug": false,
